type Dog {
    id: ID!
    name: String!
    breed: String!
    origin: String!
}

type Query{
    findAll: [Dog]!
    findBreedById: String!
    findAllBreed: [String]!
    findAllNames: [String]!
}

type Mutation{
    newDog(name: String!, breed: String!, origin: String!) : Dog!
    deleteDogBreed(id:ID!) : Boolean
    updateDogName(newName: String!, id:ID!) : Dog!
}